{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "no-unused-vars": "off",
    "import/no-unresolved": "error"
  }
}
